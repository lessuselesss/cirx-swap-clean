    class="connect-button"
    @click="openConnection"
    :variant="isConnected  ? 'outline' : 'solid'"
    :loading="isConnected && !address"
  >
    <template v-if="isConnected">
     {{   address ? shortenAddress(address) : "Loading..." }}
    </template>
    <template v-else> Connect </template>
  </UButton>
</template>
